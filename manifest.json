{  
  "name": "autohome boost",  
  "version": "0.1.0",  
  "description": "为汽车之家增加一些缺失的功能, 比如车系的收藏和车型的收藏.",  
  "page_action": {  
    "default_icon": "icon.png" ,
    "default_title": "autohome boost",
    "default_popup": "my_account.html"
  }, 
  "permissions": [
    "webNavigation",
    "tabs",
    "*://*.autohome.com.cn/*",
    "http://localhost:8080/*",
    "http://*/"
  ],

   "content_scripts": [
    {
      "matches": [ "*://www.autohome.com.cn/*/" ],
      "exclude_globs" : ["*//www.autohome.com.cn/*/?*?/"],
      "run_at" : "document_end",
      "css" : [ "series_fav.css" ],
      "js": [ "series_fav.js" ]
    },
    {
      "matches": [ "*://www.autohome.com.cn/spec/*/" ],
      "exclude_globs" : ["*//www.autohome.com.cn/spec/*/?*?"],
      "run_at" : "document_end",
      "css" : [ "spec_fav.css" ],
      "js": [ "spec_fav.js" ]
    }
  ],

  "web_accessible_resources" : [
    "series_fav_clicked.js",
    "spec_fav_clicked.js"
  ],

  "manifest_version": 2
} 
